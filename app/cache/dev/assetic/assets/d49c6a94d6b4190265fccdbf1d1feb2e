$(document).ready(function(){var b={show_button:"ui-icon-search",add_button:"ui-icon-plusthick",del_button:"ui-icon-closethick",edit_button:"ui-icon-pencil"};$.each(b,function(d,c){$("."+d).button({text:false,icons:{primary:c}})});$(".toolbar a").css("margin","0 0");function a(c){alert("")}});jQuery.mesages_validation=function(a){if(a.code){$.achtung({message:"El dato fue guardado",timeout:5})}else{$.each(a.msg,function(b,c){$.achtung({message:'Campo "'+b+'" : '+c,timeout:5})})}return a.code};